{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-tabs": 0,
        "dot-notation": 0,
        "default-case": 0,
        "class-methods-use-this": 0,
        "import/no-unresolved": 0,
        "no-param-reassign": 0,
        "import/no-cycle": 0
    }
}